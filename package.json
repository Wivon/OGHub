{
  "name": "og-hub",
  "productName": "OG Hub",
  "version": "0.3.9",
  "description": "Free up Space in your Taskbar",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder build --win --publish never",
    "deploy": "electron-builder build --win --publish always"
  },
  "build": {
    "appId": "og-hub",
    "win": {
      "icon": "src/img/logoX512.ico"
    },
    "nsis": {
      "installerIcon": "src/img/setup_logoX512.ico",
      "uninstallerIcon": "src/img/uninstall_logoX512.ico",
      "uninstallDisplayName": "uninstall OG Hub",
      "oneClick": false,
      "shortcutName": "OG Hub",
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [
    "OG hub"
  ],
  "author": "Wivon Hub",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wivon/OGHub.git"
  },
  "license": "ISC",
  "devDependencies": {
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7"
  },
  "dependencies": {
    "electron-updater": "^4.3.9",
    "exe-icon-extractor": "^1.0.8",
    "icon-extractor": "^1.0.3",
    "installed-win-apps": "^1.0.12",
    "path": "^0.12.7"
  }
}
